<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TesseractOcrMauiTestApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TesseractOcrMauiTestApp"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent Title="Home"
                      ContentTemplate="{DataTemplate local:MainPage}"
                      Route="MainPage" />
        
        <ShellContent Title="Visual Iterator"
                      ContentTemplate="{DataTemplate local:VisualOcrPage}"
                      Route="VisualOcrPage"
                      />
        
        <ShellContent Title="Text Block Iterator"
                      ContentTemplate="{DataTemplate local:TextIteratorPage}"
                      Route="BlockOcrPage"
                      />
    </TabBar>
 

</Shell>
